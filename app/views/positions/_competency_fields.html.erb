<fieldset> 
  <%= f.text_field :competencies, :style => "width: 600pt;", :class => 'competency' %>
  <%= f.select(:competency_cpv, [['1'],['2'],['3'],['4']], { :prompt => "0"}, :style => "width: 32pt;") %>
  <%= f.select(:competency_value, [['1'],['2'],['3'],['4']], { :prompt => "0"}, :style => "width: 32pt;") %>
  <%= f.select(:competency_options, [['F'],['M'],['L'],['P']], { :prompt => "TBD"}, :style => "width: 40pt;") %>
  <%= f.hidden_field :_destroy %>
  <%= link_to "remove", '#', class: "remove_fields" %>
</fieldset>
