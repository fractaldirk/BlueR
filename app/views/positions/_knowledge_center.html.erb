<h1>Knowledge Center</h1>

<table class="table"">
  <tr>
    <th>Idea title</th>
    <th>Created by</th>
    <th>Created at</th>
  </tr>
<% @posts.each do |post| %>
  <tr>
    <td><%= link_to post.idea_title, post %></td>
    <td><%= post.idea_name %></td>
    <td><%= post.created_at.strftime("%d %B %Y") %></td>
  </tr>
</table>
<% end %>
